[% PROCESS header.tt %]
<div class="container-fluid">
	<div class="banners ">
		<div class="row">
			<div class="span4">
				<a href="/"><img  height="53" src="/static/images/logo_onde_acontece.short.jpg" /></a>
			</div>
			<div class="span12 sub_menu">
